config_version=4

[application]

config/name="I Will Recover"
run/main_scene="res://Scenes/Screens/Menu.tscn"
boot_splash/image="res://Splash.png"
boot_splash/bg_color=Color( 1, 0.984314, 0.854902, 1 )
config/icon="res://icon.png"

[autoload]

Global="*res://Scripts/Autoload/Global.gd"
PauseMenu="*res://Scenes/PauseMenu.tscn"
FadeScreen="*res://Scenes/FadeScreen.tscn"
AudioController="*res://Scenes/AudioController.tscn"
FpsLabel="*res://Scenes/FpsLabel.tscn"
SilentWolf="*res://addons/silent_wolf/SilentWolf.gd"
ConfigureSilentWolf="*res://Scripts/Autoload/ConfigureSilentWolf.gd"
