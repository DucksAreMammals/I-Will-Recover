[display]

window/size/width=400
window/size/height=225
window/size/test_width=1200
window/size/test_height=675
window/stretch/mode="2d"
window/stretch/aspect="keep"
window/integer_resolution_handler/base_width=400
window/integer_resolution_handler/base_height=225

[editor_plugins]

enabled=PoolStringArray(  )

[gui]

theme/custom="res://default_theme.tres"
theme/custom_font="res://Fonts/LiberationSans.tres"

